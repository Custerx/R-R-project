{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "max-len": [1, 120, 2, {"ignoreComments": true}]
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"]
    ,
    "env": {
        "browser": true,
        "node": true
    }
}
